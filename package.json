{
  "name": "chicken",
  "version": "1.0.0",
  "description": "chicken",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:buhi/chicken.git"
  },
  "scripts": {
    "version": "npm run build:prod && git add ./build/current",
    "postversion": "git push && git push --tags",
    "make:prod": "NODE_ENV=production webpack --bail --progress --profile",
    "make:prerelease": "NODE_ENV=prerelease webpack --bail --progress --profile",
    "dev": "NODE_ENV=development node index.js",
    "start": "pm2 startOrRestart ecosystem.config.js --env=production",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "build:prod": "npm run make:prod && (rm -rf build/current || true) && mv build/`date +'%Y-%m-%d-production'` build/current && cp -R ./assets ./build/current ",
    "build:prerelease": "npm run make:prerelease && (rm -rf build/prerelease || true) && mv build/`date +'%Y-%m-%d-test'` build/prerelease"
  },
  "dependencies": {
    "express": "^4.14.0",
    "pm2": "^2.4.2"
  },
  "keywords": [
    "chicken"
  ],
  "author": "buhi",
  "license": "MIT",
  "devDependencies": {
    "@types/material-ui": "0.17.18",
    "@types/node": "6.0.53",
    "@types/react-dom": "15.5.1",
    "@types/react-redux": "4.4.47",
    "@types/react-router-redux": "4.0.43",
    "@types/redux-form": "7.0.1",
    "ag-grid-material-preset": "git+ssh://git@gitlab.ucloudadmin.com:buhi/ag-grid-material-preset.git",
    "antd": "^2.12.7",
    "awesome-typescript-loader": "3.2.1",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "guguder": "git+ssh://git@gitlab.ucloudadmin.com:buhi/guguder.git",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "4.0.0-rc.2",
    "intl": "^1.2.5",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "normalize.css": "^5.0.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "react-addons-perf": "^15.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-jss": "6.1.1",
    "react-redux": "4.4.6",
    "react-router-dom": "4.0.0",
    "react-tap-event-plugin": "2.0.0",
    "react-transition-group": "^1.1.*",
    "redux-restful-resource": "*",
    "redux-schema-form": "buhichan/redux-schema-form#v2",
    "reselect": "^3.0.1",
    "style-loader": "^0.13.1",
    "sw-precache-webpack-plugin": "^0.9.1",
    "tinycolor2": "^1.4.1",
    "typescript": "2.4.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "2.5.*",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-md5-hash": "*",
    "whatwg-fetch": "^2.0.1"
  }
}
